{
    "name": "shunhua/laravel-mqtt",
    "description": "A simple Laravel Library to connect/publish/subscribe to MQTT broker",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Salman Zafar",
            "email": "salmanzafar949@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "laravel5",
        "laravel6",
        "laravel7",
        "laravel8",
        "laravel-5",
        "laravel-6",
        "laravel-7",
        "laravel-8",
        "mqtt-laravel",
        "mqtt-laravel-publisher",
        "mqtt-laravel-subscriber",
        "laravel mqtt library",
        "laravel-mqtt",
        "laravel mqtt",
        "mqtt laravel",
        "php",
        "php 5.4",
        "php 7.0",
        "php 7.1",
        "php 7.2",
        "php 7.3",
        "php 7.4",
        "php 8"
    ],
    "require": {
        "php": "^7.0 || ^7.1 || ^7.2 || ^7.3 || ^7.4 || ^8.0"
    },
    "require-dev": {
        "laravel/framework": "^5.5 || ^6.0 || ^7.0 || ^8.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Salman\\Mqtt\\": "src/"
        },
        "files": [
            "src/Helper/Helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Salman\\Mqtt\\MqttServiceProvider"
            ],
            "aliases": {
                "Mqtt": "Salman\\Mqtt\\Facades\\Mqtt"
            }
        }
    }
}
